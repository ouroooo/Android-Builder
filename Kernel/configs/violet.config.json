[
  {
    "kernelSource": {
      "name": "QUJIKERNEL-violet",
      "repo": "https://github.com/ouroooo/android_kernel_xiaomi_sm6150",
      "branch": "lineage-22.2",
      "device": "violet",
      "defconfig": "defconfig"
    },
   "toolchains": [
  {
    "name": "proton-clang",
    "repo": "https://github.com/kdrag0n/proton-clang",
    "branch": "master",
    "binaryEnv": ["./bin"]
  }
],
    "enableCcache": false,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "proton-clang/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "proton-clang/bin/arm-linux-gnueabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
